  {
    "name": "Typography",
    "settings": [
      {
        "type": "header",
        "content": "Headings"
      },
      {
        "type": "font_picker",
        "id": "heading_font",
        "default": "helvetica_n4",
        "label": "Font",
        "info": "Selecting a different font can affect the speed of your store."
      },
      {
        "type": "range",
        "id": "heading_scale",
        "min": 100,
        "max": 150,
        "step": 5,
        "unit": "%",
        "label": "Font size scale",
        "default": 100
      },
      {
        "type": "checkbox",
        "id": "uppercase_heading",
        "label": "Display headings in uppercase",
        "default": true
      },
      {
        "type": "header",
        "content": "Body"
      },
      {
        "type": "font_picker",
        "id": "text_font",
        "default": "helvetica_n4",
        "label": "Font",
        "info": "Selecting a different font can affect the speed of your store."
      },
      {
        "type": "range",
        "id": "body_scale",
        "min": 100,
        "max": 150,
        "step": 5,
        "unit": "%",
        "label": "Font size scale",
        "default": 100
      },
      {
        "type": "range",
        "id": "base_text_font_size",
        "label": "Base size",
        "min": 13,
        "max": 17,
        "unit": "px",
        "default": 14
      }
    ]
  },
  {
    "name": "Search",
    "settings": [
      {
        "type": "select",
        "id": "search_mode",
        "label": "Search only...",
        "options": [
          {
            "value": "product",
            "label": "Products only"
          },
          {
            "value": "product,page",
            "label": "Products and pages"
          },
          {
            "value": "product,article",
            "label": "Products and articles"
          },
          {
            "value": "product,article,page",
            "label": "Products, articles and pages"
          }
        ],
        "default": "product,article"
      }
    ]
  },
  {
    "name": "Cart",
    "settings": [
      {
        "type": "select",
        "id": "cart_type",
        "options": [
          {
            "value": "drawer",
            "label": "Drawer"
          },
          {
            "value": "page",
            "label": "Page"
          }
        ],
        "default": "drawer",
        "label": "Cart type"
      },
      {
        "type": "checkbox",
        "id": "cart_enable_notes",
        "label": "Enable cart note",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "cart_show_free_shipping_threshold",
        "label": "Show free shipping minimum amount",
        "info": "Make sure that you have properly configured your [shipping rates](/admin/settings/shipping).",
        "default": false
      },
      {
        "type": "text",
        "id": "cart_free_shipping_threshold",
        "label": "Free shipping minimum amount",
        "info": "Only write a number, no letters",
        "default": "50"
      }
    ]
  },
